{
    "name": "ndeet/lnd-grpc-client",
    "description": "gRPC client for Bitcoin Lightning network LND implementation. The code is generated from rpc.proto.",
    "keywords": ["lightning", "grpc", "lnd", "bitcoin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Tasch",
            "email": "andy.tasch+packagist@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ndeet/php-lnd-grpc-client/issues"
    },
    "type": "library",
    "require": {
        "php": ">=5.6",
        "ext-grpc": ">=0.1.0",
        "grpc/grpc": "^v1.1.0",
        "google/protobuf": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Lnrpc\\": "src/Lnrpc/",
            "Google\\": "src/Google/",
            "GPBMetadata\\": "src/GPBMetadata/"
        }
    }
}
